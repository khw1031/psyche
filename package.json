{
  "name": "psyche",
  "version": "1.0.0",
  "description": "AI 기반 감정 일기 분석을 통해 개인의 심리적 성장과 인지적 재구성을 지원하는 24시간 접근 가능한 멘탈 헬스케어 서비스",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "supabase": "2.30.4",
    "turbo": "^1.12.4",
    "typescript": "^5.3.3"
  },
  "packageManager": "yarn@4.9.2",
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=4.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "author": "Psyche Team",
  "license": "MIT"
}
